{
	"name": "args-flagify",
	"version": "2.2.0",
	"description": "Node.js CLI helper with zero dependencies",
	"main": "index.js",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"scripts": {
		"test": "xo && ava"
	},
	"author": "Aleksandar Stojanoski",
	"license": "MIT",
	"homepage": "https://github.com/acestojanoski/args-flagify#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/acestojanoski/args-flagify.git"
	},
	"bugs": {
		"url": "https://github.com/acestojanoski/args-flagify/issues"
	},
	"keywords": [
		"cli",
		"flags",
		"args",
		"argv",
		"flagify"
	],
	"devDependencies": {
		"ava": "^3.5.0",
		"xo": "^0.26.1"
	},
	"xo": {
		"prettier": true,
		"ignores": [
			"**/*.ts"
		],
		"rules": {
			"unicorn/no-process-exit": "off",
			"default-case": "off",
			"no-unused-vars": "warn",
			"no-negated-condition": "off",
			"capitalized-comments": "off"
		}
	},
	"ava": {
		"files": [
			"test/**/*.test.js"
		]
	}
}
